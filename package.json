{
  "name": "@jurajkavka/vue-hamburger-button",
  "version": "1.0.2",
  "description": "Hamburger button for the Vue apps.",
  "author": "Juraj Kavka <jurajkavka@gmail.com>",
  "license": "MIT",
  "main": "dist/hamburger-button.ssr.js",
  "module": "dist/hamburger-button.esm.js",
  "unpkg": "dist/hamburger-button.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JurajKavka/vue-hamburger-button.git"
  },
  "files": [
    "dist/*",
    "src/**/*.scss",
    "src/**/*.vue",
    "!src/serve-dev.*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "vue-component",
    "vue-hamburger-button",
    "hamburger-button",
    "hamburger-menu",
    "hamburger",
    "vue-sidebar",
    "vue-sidebar-menu",
    "sidebar",
    "sidebar-menu"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/serve-dev.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "lint:script": "eslint --ext .vue,.js src/",
    "lint:style": "stylelint  \"src/(**)*.{scss,vue}\"",
    "lint": "npm run lint:script; npm run lint:style",
    "test:unit": "vue-cli-service test:unit",
    "test": "npm run test:unit"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-replace": "^2.3.1",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-unit-jest": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-jest": "^25.1.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^25.1.0",
    "minimist": "^1.2.5",
    "node-sass": "^4.13.1",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "5.1.1",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
